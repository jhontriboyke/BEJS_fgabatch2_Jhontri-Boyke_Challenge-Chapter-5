{
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
        "/node_modules",
        "/src/models",
        "/src/middlewares",
        "/src/controllers"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
        "global": {
            "branches": 20,
            "functions": 30,
            "lines": 50,
            "statements": 50
        }
    },
    "mapCoverage": true,
    "transformIgnorePatterns": [
        "<rootDir>/node_modules/(?!@ionic-native|@ionic|@ngrx|angular2-ui-switch|angularfire2|jest-cli)"
    ],
    "verbose": false
}